<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity5">


    <Button
        android:id="@+id/btnVolverConLlave"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:onClick="conLlave"
        android:text="@string/btn_volver_conllave" />

    <Button
        android:id="@+id/btnVolverSinLlave"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/btnVolverConLlave"
        android:layout_centerHorizontal="true"
        android:onClick="sinLlave"
        android:text="@string/btn_volver_sinllave" />


</RelativeLayout>